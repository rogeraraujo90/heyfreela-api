{
  "name": "heyfreela-api",
  "version": "1.0.0",
  "description": "NodeJS API for HeyFreela features consumption",
  "main": "src/app.ts",
  "scripts": {
    "ci": "npm run lint && npm test",
    "dev-start": "ts-node-dev -r tsconfig-paths/register --transpile-only src/app.ts",
    "eslint": "eslint \"**/*.{ts,tsx}\"",
    "lint": "npm run prettier & npm run eslint & npm run ts-check",
    "prettier": "prettier --check .",
    "test": "echo \"Warning: no tests yet\"",
    "ts-check": "tsc --noemit",
    "typeorm": "node -r tsconfig-paths/register --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rogeraraujo90/heyfreela-api.git"
  },
  "keywords": [
    "webapp",
    "business",
    "technology",
    "development"
  ],
  "author": "Roger Araujo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rogeraraujo90/heyfreela-api/issues"
  },
  "homepage": "https://github.com/rogeraraujo90/heyfreela-api#readme",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.11.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "2.5.1",
    "ts-node": "10.4.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.14.0",
    "typescript": "4.5.2"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0",
    "typeorm": "0.2.45"
  }
}
